<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="540.0" prefWidth="960.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scrumsystem.ProjectController">
   <children>
      <Label fx:id="projectTitle" alignment="TOP_CENTER" prefHeight="31.0" prefWidth="389.0" text="PROJECT_TITLE" />
      <Label fx:id="totalTasks" alignment="TOP_CENTER" prefHeight="0.0" prefWidth="254.0" text="NUM_TASKS" />
      <HBox fx:id="columnsHBox" prefHeight="100.0" spacing="50.0">
         <padding>
            <Insets left="50.0" />
         </padding></HBox>
      <Label fx:id="columnStatus" text="STATUS_COL_SELECT" />
      <HBox alignment="TOP_CENTER" prefHeight="0.0" prefWidth="920.0" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#addColumn" text="Add Column" />
            <Button mnemonicParsing="false" onAction="#addTaskToColumn" prefWidth="147.0" text="Add Task" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#returnToDashboard" text="Back" />
         </children>
         <padding>
            <Insets left="40.0" />
         </padding>
      </HBox>
      
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>

</VBox>
