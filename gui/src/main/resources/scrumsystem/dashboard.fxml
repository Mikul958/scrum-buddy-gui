<VBox alignment="CENTER" prefHeight="432.0" prefWidth="540.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scrumsystem.DashboardController">
   <children>
      <Label fx:id="lbl_username" alignment="CENTER" prefHeight="50.0" prefWidth="150.0" text="Dashboard" />
      <HBox fx:id="dashboardHBox" prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox fx:id="projectVBox" prefHeight="200.0" prefWidth="100.0" />
         </children>
      </HBox>
      <Button mnemonicParsing="false" onAction="#logout" text="Back" />
      
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <stylesheets>
      <URL value="@styles.css" />
   </stylesheets>

</VBox>
